<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Main</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet"
          integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
</head>
<body class="m-auto text-center py-4">

<div>
    <a class="btn btn-primary" href="/" role="button">Main Manu</a>
    <a class="btn btn-primary" href="/books" role="button">Books</a>
    <a class="btn btn-primary" href="/authors" role="button">Authors</a>
</div>
<div class="d-flex m-auto text-center py-4 bg-body-tertiary w-75">
    <table class="table table-dark">
        <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Full name</th>
            <th scope="col">Date of birth</th>
            <th scope="col">Book name</th>
            <th scope="col">ISBN</th>
            <th scope="col">Book published date</th>
        </tr>
        </thead>

        <tbody>
        <% result.map(res => { %>
            <tr>
                <th scope="row"><%- res.id%></th>
                <td><%- res.fullName %></td>
                <td><%- res.dateOfBirth %></td>
                <td><%- res.bookName %></td>
                <td><%- res.ISBN %></td>
                <td><%- res.publishedDate %></td>
            </tr>
        <% }) %>

        </tbody>
    </table>
</div>


